{
  "extends": [
    "@rocketseat/eslint-config/react",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "simple-import-sort",
    "import",
    "prettier"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
